{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "airu": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/airu",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/assets/vendors/bootstrap/dist/css/bootstrap.css",
              "src/assets/vendors/font-feathericons/dist/feather.css",
              "src/assets/vendors/font-awesome/css/font-awesome.min.css",
              "src/assets/vendors/font-linearicons/style.css",
              "src/assets/vendors/font-icomoon/style.css",
              "src/assets/vendors/perfect-scrollbar/css/perfect-scrollbar.css",
              "src/assets/vendors/chart.js/dist/Chart.min.css",
              "src/assets/vendors/jqvmap/dist/jqvmap.min.css",
              "src/assets/vendors/c3/c3.min.css",
              "src/assets/vendors/tempus-dominus-bs4/build/css/tempusdominus-bootstrap-4.min.css",
              "src/assets/datatables.min.css",
              "src/assets/vendors/fullcalendar/dist/fullcalendar.min.css",
              "src/assets/vendors/owl.carousel/dist/assets/owl.carousel.min.css",
              "src/assets/vendors/ionrangeslider/css/ion.rangeSlider.css",
              "src/assets/vendors/bootstrap-sweetalert/dist/sweetalert.css",
              "src/assets/vendors/nprogress/nprogress.css",
              "src/assets/vendors/summernote/dist/summernote.css",
              "src/assets/vendors/dropify/dist/css/dropify.min.css",
              "src/assets/vendors/jquery-steps/demo/css/jquery.steps.css",
              "src/assets/vendors/select2/dist/css/select2.min.css",
              "src/assets/vendors/bootstrap-select/dist/css/bootstrap-select.min.css",
              "src/assets/components/vendors/style.css",
              "src/assets/components/core/style.css",
              "src/assets/components/widgets/style.css",
              "src/assets/components/system/style.css",
              "src/assets/components/menu-left/style.css",
              "src/assets/components/menu-top/style.css",
              "src/assets/components/footer/style.css",
              "src/assets/components/topbar/style.css",
              "src/assets/components/subbar/style.css",
              "src/assets/components/sidebar/style.css",
              "src/assets/components/apps/style.css",
              "src/assets/components/extra-apps/style.css",
              "src/assets/components/ecommerce/style.css",
              "src/styles.css"
            ],
            "scripts": [
              "src/assets/vendors/jquery/dist/jquery.min.js",
              "src/assets/vendors/popper.js/dist/umd/popper.js",
              "src/assets/vendors/bootstrap/dist/js/bootstrap.js",
              "src/assets/vendors/jquery-mousewheel/jquery.mousewheel.min.js",
              "src/assets/vendors/perfect-scrollbar/js/perfect-scrollbar.jquery.js",
              "src/assets/vendors/chartist/dist/chartist.min.js",
              "src/assets/vendors/chart.js/dist/Chart.min.js",
              "src/assets/vendors/jqvmap/dist/jquery.vmap.min.js",
              "src/assets/vendors/jqvmap/dist/maps/jquery.vmap.usa.js",
              "src/assets/vendors/chartist-plugin-tooltip/dist/chartist-plugin-tooltip.min.js",
              "src/assets/vendors/d3/d3.min.js",
              "src/assets/vendors/d3-dsv/dist/d3-dsv.js",
              "src/assets/vendors/d3-time-format/dist/d3-time-format.js",
              "src/assets/vendors/c3/c3.min.js",
              "src/assets/vendors/peity/jquery.peity.min.js",
              "src/assets/datatables.min.js",
              "src/assets/vendors/editable-table/mindmup-editabletable.js",
              "src/assets/vendors/moment/min/moment.min.js",
              "src/assets/vendors/tempus-dominus-bs4/build/js/tempusdominus-bootstrap-4.min.js",
              "src/assets/vendors/fullcalendar/dist/fullcalendar.min.js",
              "src/assets/vendors/owl.carousel/dist/owl.carousel.min.js",
              "src/assets/vendors/ionrangeslider/js/ion.rangeSlider.min.js",
              "src/assets/vendors/remarkable-bootstrap-notify/dist/bootstrap-notify.min.js",
              "src/assets/vendors/bootstrap-sweetalert/dist/sweetalert.min.js",
              "src/assets/vendors/nprogress/nprogress.js",
              "src/assets/vendors/summernote/dist/summernote.min.js",
              "src/assets/vendors/nestable/jquery.nestable.js",
              "src/assets/vendors/jquery-typeahead/dist/jquery.typeahead.min.js",
              "src/assets/vendors/autosize/dist/autosize.min.js",
              "src/assets/vendors/bootstrap-show-password/dist/bootstrap-show-password.min.js",
              "src/assets/vendors/dropify/dist/js/dropify.min.js",
              "src/assets/vendors/html5-form-validation/dist/jquery.validation.min.js",
              "src/assets/vendors/jquery-steps/build/jquery.steps.min.js",
              "src/assets/vendors/jquery-mask-plugin/dist/jquery.mask.min.js",
              "src/assets/vendors/select2/dist/js/select2.full.min.js",
              "src/assets/vendors/bootstrap-select/dist/js/bootstrap-select.min.js",
              "src/assets/vendors/moment/min/moment.min.js",
              "src/assets/components/core/index.js",
              "src/assets/components/menu-left/index.js",
              "src/assets/components/menu-top/index.js",
              "src/assets/components/sidebar/index.js",
              "src/assets/components/topbar/index.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "airu:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "airu:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "airu:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "airu:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "airu:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "airu"
}